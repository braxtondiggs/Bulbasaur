{
  "name": "bulbasaur",
  "version": "3.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "dev": "ng serve --open",
    "build": "ng build --configuration production",
    "build:dev": "ng build",
    "build:stats": "ng build --configuration production --stats-json",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "test:watch": "jest --runInBand --watchAll --coverage",
    "test:ci": "jest --runInBand --coverage --watchAll=false",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "analyze": "npm run build:stats && webpack-bundle-analyzer dist/stats.json",
    "e2e": "ng e2e",
    "e2e:headless": "cypress run",
    "e2e:open": "cypress open",
    "clean": "rm -rf dist coverage .angular",
    "pretest:ci": "npm run clean",
    "deploy": "firebase deploy --only hosting",
    "serve:dist": "npx http-server dist -p 4200 -c-1",
    "check": "npm run lint && npm run test:ci && npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.14",
    "@angular/common": "^19.2.14",
    "@angular/compiler": "^19.2.14",
    "@angular/core": "^19.2.14",
    "@angular/fire": "^19.2.0",
    "@angular/forms": "^19.2.14",
    "@angular/platform-browser": "^19.2.14",
    "@angular/platform-browser-dynamic": "^19.2.14",
    "@angular/router": "^19.2.14",
    "@angular/service-worker": "^19.2.14",
    "@ng-icons/core": "^23.3.0",
    "@ng-icons/feather-icons": "^23.3.0",
    "@ngx-loading-bar/core": "^7.0.0",
    "@ngx-loading-bar/http-client": "^7.0.0",
    "daisyui": "^4.12.24",
    "dayjs": "^1.11.14",
    "firebase": "^9.10.0",
    "highcharts": "^12.3.0",
    "highcharts-angular": "^4.0.1",
    "rxjs": "~7.8.0",
    "theme-change": "^2.5.0",
    "tslib": "^2.4.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1902.15",
    "@angular-eslint/builder": "^19.8.1",
    "@angular-eslint/eslint-plugin": "^19.8.1",
    "@angular-eslint/eslint-plugin-template": "^19.8.1",
    "@angular-eslint/schematics": "^19.8.1",
    "@angular-eslint/template-parser": "^19.8.1",
    "@angular/build": "^19.2.15",
    "@angular/cli": "^19.2.15",
    "@angular/compiler-cli": "^19.2.14",
    "@angular/language-service": "^19.2.14",
    "@briebug/cypress-schematic": "^4.4.0",
    "@cypress/webpack-preprocessor": "^5.12.2",
    "@ngneat/spectator": "^14.0.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "autoprefixer": "^10.4.21",
    "cypress": "^10.7.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.6.1",
    "jest-watch-typeahead": "^2.1.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-loader": "9.3.1",
    "typescript": "~5.8.3",
    "webpack": "^5.101.3",
    "webpack-bundle-analyzer": "^4.6.1"
  }
}
