{
  "name": "bulbasaur",
  "version": "3.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve --open",
    "build": "ng build --configuration production",
    "build:dev": "ng build",
    "build:stats": "ng build --configuration production --stats-json",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "test:watch": "jest --runInBand --watchAll --coverage",
    "test:ci": "jest --runInBand --coverage --watchAll=false",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "analyze": "npm run build:stats && webpack-bundle-analyzer dist/stats.json",
    "e2e": "ng e2e",
    "e2e:headless": "cypress run",
    "e2e:open": "cypress open",
    "clean": "rm -rf dist coverage .angular",
    "prebuild": "npm run clean",
    "pretest:ci": "npm run clean",
    "deploy": "firebase deploy --only hosting",
    "serve:dist": "npx http-server dist -p 4200 -c-1",
    "check": "npm run lint && npm run test:ci && npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/fire": "^7.4.1",
    "@angular/forms": "^15.2.10",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@angular/service-worker": "^15.2.10",
    "@ng-icons/core": "^23.3.0",
    "@ng-icons/feather-icons": "^23.3.0",
    "@ngx-loading-bar/core": "^6.0.2",
    "@ngx-loading-bar/http-client": "^6.0.2",
    "daisyui": "^5.0.50",
    "dayjs": "^1.11.5",
    "firebase": "^9.10.0",
    "highcharts": "^10.2.1",
    "highcharts-angular": "^3.0.0",
    "lodash-es": "^4.17.21",
    "ng-lazyload-image": "^9.1.3",
    "rxjs": "~7.5.6",
    "theme-change": "^2.5.0",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1502.11",
    "@angular-devkit/build-angular": "^15.2.11",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^15.2.11",
    "@angular/compiler-cli": "^15.2.10",
    "@angular/language-service": "^15.2.10",
    "@briebug/cypress-schematic": "^4.4.0",
    "@cypress/webpack-preprocessor": "^5.12.2",
    "@ngneat/spectator": "^14.0.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jest": "^29.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.21",
    "cypress": "^10.7.0",
    "eslint": "^8.57.1",
    "jest": "^28.0.0",
    "jest-preset-angular": "^12.2.2",
    "jest-watch-typeahead": "^2.1.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-loader": "9.3.1",
    "typescript": "~4.9.5",
    "webpack": "^5.101.3",
    "webpack-bundle-analyzer": "^4.6.1"
  }
}
