{
  "name": "bulbasaur",
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "dev": "ng serve --open",
    "build": "ng build --configuration production",
    "build:dev": "ng build",
    "build:ssr": "ng build && npm run compile:server",
    "build:ssr:prod": "ng build --configuration production && npm run compile:server",
    "compile:server": "tsc -p tsconfig.server.json",
    "serve:ssr": "npm run build:ssr && node dist/server/server.js",
    "serve:ssr:prod": "npm run build:ssr:prod && node dist/server/server.js",
    "build:stats": "ng build --configuration production --stats-json",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "test:watch": "jest --runInBand --watchAll --coverage",
    "test:ci": "jest --runInBand --coverage --watchAll=false",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "npm run build:stats && npx esbuild-visualizer --metadata dist/stats.json --open",
    "clean": "rm -rf dist coverage .angular",
    "pretest:ci": "npm run clean",
    "deploy": "firebase deploy",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:ssr": "npm run build:ssr:prod && firebase deploy",
    "deploy:preview": "firebase hosting:channel:deploy preview --expires 7d",
    "emulators": "firebase emulators:start",
    "emulators:hosting": "firebase emulators:start --only hosting",
    "emulators:functions": "firebase emulators:start --only functions",
    "serve:dist": "npx http-server dist -p 4200 -c-1",
    "check": "npm run format:check && npm run lint && npm run test:ci && npm run build",
    "predeploy": "npm run check",
    "predeploy:hosting": "npm run build",
    "predeploy:functions": "cd functions && npm run check"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.2.2",
    "@angular/compiler": "^20.2.2",
    "@angular/core": "^20.2.2",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.2.2",
    "@angular/platform-browser": "^20.2.2",
    "@angular/platform-browser-dynamic": "^20.2.2",
    "@angular/platform-server": "^20.2.2",
    "@angular/router": "^20.2.2",
    "@angular/service-worker": "^20.2.2",
    "@angular/ssr": "^20.2.1",
    "@ng-icons/core": "^32.1.0",
    "@ng-icons/feather-icons": "^32.1.0",
    "@ngx-loading-bar/core": "^7.0.0",
    "@ngx-loading-bar/http-client": "^7.0.0",
    "daisyui": "^5.0.53",
    "dayjs": "^1.11.14",
    "express": "^4.21.1",
    "firebase": "^12.1.0",
    "highcharts": "^12.3.0",
    "highcharts-angular": "^5.1.0",
    "rxjs": "~7.8.0",
    "theme-change": "^2.5.0",
    "tslib": "^2.4.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.2002.1",
    "@angular-eslint/builder": "^20.2.0",
    "@angular-eslint/eslint-plugin": "^20.2.0",
    "@angular-eslint/eslint-plugin-template": "^20.2.0",
    "@angular-eslint/schematics": "^20.2.0",
    "@angular-eslint/template-parser": "^20.2.0",
    "@angular/build": "^20.2.1",
    "@angular/cli": "^20.2.1",
    "@angular/compiler-cli": "^20.2.2",
    "@angular/language-service": "^20.2.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@ngneat/spectator": "^21.0.1",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "esbuild-visualizer": "^0.7.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.1.1",
    "jest-preset-angular": "^15.0.0",
    "jest-watch-typeahead": "^3.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.12",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "webpack-cli": "^6.0.1"
  }
}
